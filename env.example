# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General > Your apps
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef

# Google Photos OAuth (NO API KEY NEEDED)
VITE_GOOGLE_PHOTOS_CLIENT_ID=your_google_client_id

# Google Calendar OAuth (NO API KEY NEEDED)
VITE_GOOGLE_CALENDAR_CLIENT_ID=your_google_calendar_client_id

# Weather API
VITE_WEATHER_API_KEY=your_weather_api_key

# Household ID for demo data
VITE_HOUSEHOLD_ID=demo-family-001
